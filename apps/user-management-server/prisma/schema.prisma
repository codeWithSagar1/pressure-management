datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Session {
  createdAt  DateTime  @default(now())
  expiryTime DateTime?
  id         String    @id @default(cuid())
  sessionId  String?
  updatedAt  DateTime  @updatedAt
  user       User?     @relation(fields: [userId], references: [id])
  userId     String?
}

model User {
  autoLogoutTime               Int?
  autoLogoutTimeCustom         Int?
  autoUnblockTime              Int?
  autoUnblockTimeCustom        Int?
  comment                      String?
  commentCustom                String?
  createdAt                    DateTime                     @default(now())
  email                        String?                      @unique
  expiryDaysNotification       Int?
  expiryDaysNotificationCustom Int?
  firstName                    String?
  id                           String                       @id @default(cuid())
  lastName                     String?
  numberOfAttempts             Int?
  numberOfAttemptsCustom       Int?
  password                     String
  passwordExpiry               Int?
  passwordExpiryCustom         Int?
  passwordText                 String?
  roles                        Json
  sessions                     Session[]
  updatedAt                    DateTime                     @updatedAt
  userLevel                    EnumUserUserLevel?
  userLevelCustomized          EnumUserUserLevelCustomized?
  username                     String                       @unique
  userName                     String?
  usernameCustom               String?
}

enum EnumUserUserLevel {
  Option1
}

enum EnumUserUserLevelCustomized {
  Option1
}
